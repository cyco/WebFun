{
  "name": "WebFun",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --production --optimize-minimize --config config/webpack.prod.js",
    "start": "webpack-dev-server --open --mode development --config config/webpack.dev.js",
    "test": "karma start config/karma.conf.js --single-run",
    "test:cont": "karma start config/karma.conf.js --watch --single-run=false",
    "test:cov": "cross-env coverage=1 karma start config/karma.conf.js --single-run=true",
    "test:cov:cont": "cross-env coverage=1 karma start config/karma.conf.js --watch --single-run=false",
    "test:cont:cov": "cross-env coverage=1 karma start config/karma.conf.js --watch --single-run=false",
    "test:acceptance": "cross-env scope=acceptance karma start config/karma.conf.js --single-run --",
    "test:unit": "cross-env coverage=1 scope=unit karma start config/karma.conf.js --single-run",
    "test:coverage": "cross-env coverage=1 scope=unit,acceptance karma start config/karma.conf.js --single-run",
    "test:unit:cont": "cross-env coverage=1 scope=unit karma start config/karma.conf.js --single-run=false --watch=true",
    "test:full": "cross-env coverage=1 scope=unit,acceptance karma start config/karma.conf.js",
    "test:full:cont": "cross-env coverage=1 scope=unit,acceptance karma start config/karma.conf.js --watch=true --single-run=false",
    "gen-coverage": "genhtml test/coverage/unit.lcov test/coverage/acceptance/lcov.info -o test/coverage/html",
    "format": "eslint --fix 'src/**/*.{js,ts,tsx,jsx}'"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current",
            "browsers": [
              "last 1 version"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/transform-regenerator",
      [
        "@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": 3,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "WebFun.JSX.render"
        }
      ]
    ]
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "detect-mobile-browser": "^5.0.0",
    "font-awesome": "^4.7.0",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@types/jasmine-matchers": "^0.2.32",
    "@types/text-encoding": "^0.0.35",
    "@types/webcomponents.js": "^0.6.35",
    "@types/webpack": "^4.41.10",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "add-matchers": "^0.6.2",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "canvas": "^2.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^1.7.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "font-loader": "^0.1.2",
    "html-webpack-plugin": "^4.0.4",
    "ignore-loader": "^0.1.2",
    "jasmine-core": "^3.5.0",
    "jsdom": "^16.2.2",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-matchers": "^4.0.2",
    "karma-sonarqube-reporter": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.2",
    "puppeteer": "^2.1.1",
    "raw-loader": "^4.0.0",
    "sass-loader": "~8.0.2",
    "shelljs": "^0.8.3",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
