{
  "name": "webfun",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "15"
  },
  "scripts": {
    "build": "webpack --mode=production --config config/webpack.prod.js",
    "start": "webpack serve --open --mode=development --config config/webpack.dev.js",
    "test": "karma start config/karma.conf.js --single-run",
    "test:cont": "karma start config/karma.conf.js --watch --single-run=false",
    "test:unit": "cross-env coverage=1 scope=unit karma start config/karma.conf.js --single-run",
    "test:unit:cont": "cross-env coverage=1 scope=unit karma start config/karma.conf.js --single-run=false --watch=true",
    "test:full": "cross-env coverage=1 scope=unit,acceptance karma start config/karma.conf.js",
    "format": "eslint --fix 'src/**/*.{js,ts,tsx,jsx}'"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current",
            "browsers": [
              "last 1 version"
            ]
          },
          "modules": "commonjs"
        }
      ]
    ],
    "plugins": [
      "@babel/transform-regenerator",
      [
        "@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": 3,
          "helpers": true,
          "useESModules": false
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "WebFun.JSX.render"
        }
      ]
    ]
  },
  "dependencies": {
    "detect-mobile-browser": "^5.0.0",
    "font-awesome": "^4.7.0",
    "karma-webpack": "^5.0.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "^7.13.5",
    "@babel/runtime-corejs3": "^7.13.7",
    "@types/emscripten": "^1.39.4",
    "@types/jasmine-matchers": "^0.2.32",
    "@types/text-encoding": "^0.0.35",
    "@types/webcomponents.js": "^0.6.35",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "add-matchers": "^0.6.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "canvas": "^2.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.0",
    "css-url-relative-plugin": "^1.0.1",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^6.0.2",
    "eslint": "7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "font-loader": "^0.1.2",
    "google-fonts-plugin": "^5.0.2",
    "html-webpack-plugin": "^5.2.0",
    "ignore-loader": "^0.1.2",
    "jasmine-core": "^3.6.0",
    "jsdom": "^16.4.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-matchers": "^4.0.2",
    "karma-sonarqube-reporter": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "^0.0.32",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "puppeteer": "^8.0.0",
    "raw-loader": "^4.0.2",
    "sass-loader": "~11.0.1",
    "shelljs": "^0.8.4",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.17",
    "tslint": "^6.1.3",
    "typescript": "^4.2.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2",
    "webpack-sources": "^2.2.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
