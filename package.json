{
  "name": "WebFun",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "build": "webpack --production --optimize-minimize --config config/webpack.prod.js",
    "start": "webpack-dev-server --open --mode development --config config/webpack.dev.js",
    "test": "karma start config/karma.conf.js --single-run",
    "test:cont": "karma start config/karma.conf.js --watch --single-run=false",
    "test:unit": "cross-env coverage=1 scope=unit karma start config/karma.conf.js --single-run",
    "test:unit:cont": "cross-env coverage=1 scope=unit karma start config/karma.conf.js --single-run=false --watch=true",
    "test:full": "cross-env coverage=1 scope=unit,acceptance karma start config/karma.conf.js",
    "gen-coverage": "genhtml test/coverage/unit.lcov test/coverage/acceptance/lcov.info -o test/coverage/html",
    "format": "eslint --fix 'src/**/*.{js,ts,tsx,jsx}'"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current",
            "browsers": [
              "last 1 version"
            ]
          },
          "modules": "commonjs"
        }
      ]
    ],
    "plugins": [
      "@babel/transform-regenerator",
      [
        "@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": 3,
          "helpers": true,
          "useESModules": false
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "WebFun.JSX.render"
        }
      ]
    ]
  },
  "dependencies": {
    "detect-mobile-browser": "^5.0.0",
    "font-awesome": "^4.7.0",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/plugin-transform-regenerator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime-corejs3": "^7.10.5",
    "@beyonk/google-fonts-webpack-plugin": "^1.2.3",
    "@types/jasmine-matchers": "^0.2.32",
    "@types/text-encoding": "^0.0.35",
    "@types/webcomponents.js": "^0.6.35",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "add-matchers": "^0.6.2",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "canvas": "^2.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^2.0.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "file-loader": "^6.0.0",
    "font-loader": "^0.1.2",
    "google-fonts-plugin": "^5.0.2",
    "html-webpack-plugin": "^4.3.0",
    "ignore-loader": "^0.1.2",
    "jasmine-core": "^3.5.0",
    "jsdom": "^16.3.0",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-matchers": "^4.0.2",
    "karma-sonarqube-reporter": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "puppeteer": "^5.1.0",
    "raw-loader": "^4.0.1",
    "sass-loader": "~9.0.2",
    "shelljs": "^0.8.4",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "ts-loader": "^8.0.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9",
    "webpack-node-externals": "^2.5.0",
    "webpack-pwa-manifest": "^4.2.0",
    "webpack-sources": "^1.4.3"
  }
}
