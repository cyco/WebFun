{
	"name": "WebFun",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"build": "webpack --production --optimize-minimize --config config/webpack.prod.js",
		"start": "webpack-dev-server --open --mode development --config config/webpack.dev.js",
		"test": "karma start config/karma.conf.js --single-run",
		"test:cont": "karma start config/karma.conf.js --watch --single-run=false",
		"test:cov": "cross-env coverage=1 karma start config/karma.conf.js --single-run=true",
		"test:cov:cont": "cross-env coverage=1 karma start config/karma.conf.js --watch --single-run=false",
		"test:cont:cov": "cross-env coverage=1 karma start config/karma.conf.js --watch --single-run=false",
		"test:acceptance": "cross-env scope=acceptance karma start config/karma.conf.js --single-run --",
		"test:unit": "cross-env coverage=1 scope=unit karma start config/karma.conf.js --single-run",
		"test:coverage": "cross-env coverage=1 scope=unit,acceptance,performance karma start config/karma.conf.js --single-run",
		"test:unit:cont": "cross-env coverage=1 scope=unit karma start config/karma.conf.js",
		"test:full": "cross-env coverage=1 scope=unit,acceptance,performance karma start config/karma.conf.js",
		"test:full:cont": "cross-env coverage=1 scope=unit,acceptance,performance karma start config/karma.conf.js --watch=true --single-run=false",
		"test:gameplay": "cross-env TOOL_NAME=gameplay yarn build-tool && node tools/bin/gameplay.bin.js",
		"test:dump-scripts": "cross-env TOOL_NAME=dump-scripts yarn build-tool && node tools/bin/dump-scripts.bin.js -d \"build/game-data/yoda.data\"",
		"test:dump-data": "cross-env TOOL_NAME=dump-data yarn build-tool && node tools/bin/dump-data.bin.js -d \"build/game-data/yoda.data\"",
		"gen-coverage": "genhtml test/coverage/unit.lcov test/coverage/acceptance/lcov.info -o test/coverage/html",
		"test:performance": "cross-env scope=performance karma start config/karma.conf.js --single-run",
		"world": "node -- tools/bin/generate-world.bin.js -v -d \"build/game-data/yoda.data\"",
		"world:validate": "node -- tools/bin/generate-world.bin.js -c -d \"build/game-data/yoda.data\" -e \"game-data/worlds.txt\"",
		"world:validate:all": "node -- tools/bin/generate-world.bin.js -c -d \"./build/game-data/yoda.data\" -e \"./game-data/full.txt\" -a",
		"build-tool": "webpack --config config/webpack.tool.js",
		"build-tool:cont": "webpack --progress --config config/webpack.tool.js --watch",
		"format": "eslint --fix 'src/**/*.{js,ts,tsx,jsx}' 'test/**/*.{js,ts,tsx,jsx}'"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "current",
						"browsers": [
							"last 1 version"
						]
					}
				}
			]
		],
		"plugins": [
			"@babel/transform-regenerator",
			[
				"@babel/plugin-transform-react-jsx",
				{
					"pragma": "WebFunJSX.render"
				}
			]
		]
	},
	"dependencies": {
		"@babel/polyfill": "^7.4.3",
		"@babel/runtime": "^7.4.3",
		"font-awesome": "^4.7.0"
	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/plugin-transform-react-jsx": "^7.3.0",
		"@babel/plugin-transform-regenerator": "^7.4.3",
		"@babel/plugin-transform-runtime": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"@types/jasmine-matchers": "^0.2.32",
		"@types/text-encoding": "^0.0.35",
		"@types/webcomponents.js": "^0.6.34",
		"@types/webpack": "^4.4.27",
		"@typescript-eslint/eslint-plugin": "^1.6.0",
		"@typescript-eslint/parser": "^1.6.0",
		"add-matchers": "^0.6.2",
		"babel-loader": "8.0.5",
		"babel-plugin-istanbul": "^5.1.1",
		"canvas": "^2.4.1",
		"clean-webpack-plugin": "^2.0.1",
		"cross-env": "^5.2.0",
		"css-loader": "^2.1.1",
		"cssnano": "^4.1.10",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-prettier": "^3.0.1",
		"file-loader": "^3.0.1",
		"font-loader": "^0.1.2",
		"fork-ts-checker-webpack-plugin": "^1.0.2",
		"html-webpack-plugin": "^3.2.0",
		"ignore-loader": "^0.1.2",
		"jasmine-core": "^3.4.0",
		"jsdom": "^14.0.0",
		"karma": "^4.0.1",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage": "^1.1.1",
		"karma-coverage-istanbul-reporter": "^2.0.5",
		"karma-firefox-launcher": "^1.1.0",
		"karma-jasmine": "^2.0.1",
		"karma-jasmine-matchers": "^4.0.1",
		"karma-sonarqube-reporter": "^1.2.3",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-webpack": "^3.0.5",
		"mini-css-extract-plugin": "^0.6.0",
		"node-sass": "4.11.0",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"prettier": "^1.17.0",
		"puppeteer": "^1.14.0",
		"raw-loader": "^2.0.0",
		"sass-loader": "~7.1.0",
		"shelljs": "^0.8.3",
		"style-loader": "^0.23.1",
		"ts-loader": "^5.3.3",
		"tslint": "^5.15.0",
		"typescript": "^3.4.3",
		"uglifyjs-webpack-plugin": "^2.1.2",
		"url-loader": "^1.1.2",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.0",
		"webpack-dev-server": "^3.3.1",
		"webpack-merge": "^4.2.1",
		"webpack-node-externals": "^1.7.2"
	}
}
